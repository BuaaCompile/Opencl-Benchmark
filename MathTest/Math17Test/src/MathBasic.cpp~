					status = clGetEventProfilingInfo(event_remquo_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_remquo_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_remquo_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_remquo_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_remquo_withDD = (end_remquo_withDD
							- start_remquo_withDD);
					if (executionTime_remquo_withDD_max
							< executionTime_remquo_withDD) {
						executionTime_remquo_withDD_max = executionTime_remquo_withDD;
					}
					if (executionTime_remquo_withDD_min
							> executionTime_remquo_withDD) {
						executionTime_remquo_withDD_min = executionTime_remquo_withDD;
					}
					executionTime_remquo_withDD_avg += executionTime_remquo_withDD;

				}
				executionTime_remquo_withDD_avg = (executionTime_remquo_withDD_avg
						- executionTime_remquo_withDD_max
						- executionTime_remquo_withDD_min) / (iterations - 2);
				size_t time_remquo_withDD=0;
				time_remquo_withDD= 64 * szForNum * vectorSize;
				float Gflops_remquo_withDD =
						(time_remquo_withDD * szGlobalWorkSize)
								/ executionTime_remquo_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_remquo_withDD,
						CL_TRUE, 0, sizeof(cl_mem), remquo_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-remquo_withDD : %f\n", Gflops_remquo_withDD);
				printf("Result-remquo_withDD   :   %f\n\n", *((float*)remquo_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_remquo_withDD > remquo_withDD_maxGflops){
						remquo_withDD_maxGflops = Gflops_remquo_withDD;
				}
			}

			//launch kernel_remquo_withoutDD
			if (!strcmp(kernelname.c_str(), "remquo_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_remquo_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[3] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_remquo_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_remquo
					status = clWaitForEvents(1, &event_remquo_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
