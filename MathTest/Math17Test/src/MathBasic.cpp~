#include "MathBasic.hpp"

// ****************************************************************************
// Method:  oclGetProgBinary
//
// Purpose:
//   Get the binary (PTX) of the program associated with the device
//
// Arguments:
//       cpProgram    OpenCL program
//       cdDevice     device of interest
//       binary       returned code
//       length       length of returned code
//
// Copyright 1993-2013 NVIDIA Corporation
//
// ****************************************************************************
inline void
oclGetProgBinary (cl_program cpProgram, cl_device_id cdDevice, char** binary, size_t* length)
{
    // Grab the number of devices associated witht the program
    cl_uint num_devices;
    clGetProgramInfo(cpProgram, CL_PROGRAM_NUM_DEVICES, sizeof(cl_uint), &num_devices, NULL);

    // Grab the device ids
    cl_device_id* devices = (cl_device_id*) malloc(num_devices * sizeof(cl_device_id));
    clGetProgramInfo(cpProgram, CL_PROGRAM_DEVICES, num_devices * sizeof(cl_device_id), devices, 0);

    // Grab the sizes of the binaries
    size_t* binary_sizes = (size_t*)malloc(num_devices * sizeof(size_t));
    clGetProgramInfo(cpProgram, CL_PROGRAM_BINARY_SIZES, num_devices * sizeof(size_t), binary_sizes, NULL);

    // Now get the binaries
    char** ptx_code = (char**) malloc(num_devices * sizeof(char*));
    for( unsigned int i=0; i<num_devices; ++i) {
        ptx_code[i]= (char*)malloc(binary_sizes[i]);
    }
    clGetProgramInfo(cpProgram, CL_PROGRAM_BINARIES, 0, ptx_code, NULL);

    // Find the index of the device of interest
    unsigned int idx = 0;
    while( idx<num_devices && devices[idx] != cdDevice ) ++idx;

    // If it is associated prepare the result
    if( idx < num_devices )
    {
        *binary = ptx_code[idx];
        *length = binary_sizes[idx];
    }

    // Cleanup
    free( devices );
    free( binary_sizes );
    for( unsigned int i=0; i<num_devices; ++i) {
        if( i != idx ) free(ptx_code[i]);
    }
    free( ptx_code );
}

// ****************************************************************************
// Method:  oclGetFirstDev
//
// Purpose:
//   Gets the id of the first device from the context
//
// Arguments:
//       cxMainContext         OpenCL context
//
// Copyright 1993-2013 NVIDIA Corporation
//
// ****************************************************************************
inline cl_device_id
oclGetFirstDev(cl_context cxMainContext)
{
    size_t szParmDataBytes;
    cl_device_id* cdDevices;

    // get the list of GPU devices associated with context
    clGetContextInfo(cxMainContext, CL_CONTEXT_DEVICES, 0, NULL, &szParmDataBytes);
    cdDevices = (cl_device_id*) malloc(szParmDataBytes);

    clGetContextInfo(cxMainContext, CL_CONTEXT_DEVICES, szParmDataBytes, cdDevices, NULL);

    cl_device_id first = cdDevices[0];
    free(cdDevices);

    return first;
}


// ****************************************************************************
// Method:  dumpPTXCode
//
// Purpose:
//
//
// Arguments:
//   ctx          context
//   prg          program
//   name         file name prefix to output to
//
// Programmer:  Gabriel Marin
// Creation:    July 14, 2009
//
// ****************************************************************************
inline bool
dumpPTXCode (cl_context ctx, cl_program prog, const char *name)
{
    std::cout << "Dumping the PTX code" << std::endl;
    size_t ptx_length;
    char* ptx_code;
    char buf[64];
    oclGetProgBinary (prog, oclGetFirstDev(ctx), &ptx_code, &ptx_length);

    FILE* ptxFile = NULL;
    sprintf (buf, "%.59s.ptx", name);
#ifdef WIN32
    fopen_s (&ptxFile, buf, "w");
#else
    ptxFile = fopen (buf,"w");
#endif
    if (ptxFile)
    {
        fwrite (ptx_code, ptx_length, 1, ptxFile);
        fclose (ptxFile);
    }
    free (ptx_code);
    return (ptx_code!=0);
}



int MathBenchmark::initialize() {
	// Call base class Initialize to get default configuration
	if (this->Benchmark::initialize() != SDK_SUCCESS)
		return SDK_FAILURE;

	streamsdk::Option* opt_n = new streamsdk::Option;
	CHECK_ALLOCATION(opt_n, "Memory Allocation failed (opt_n)");

	opt_n->_sVersion = "N";
	opt_n->_lVersion = "N";
	opt_n->_description = "Number of iterations for kernel execution";
	opt_n->_type = streamsdk::CA_ARG_INT;
	opt_n->_value = &iterations;

	sampleArgs->AddOption(opt_n);
	delete opt_n;

	streamsdk::Option* opt_f = new streamsdk::Option;
	CHECK_ALLOCATION(opt_f, "Memory Allocation failed (opt_f)");

	opt_f->_sVersion = "F";
	opt_f->_lVersion = "F";
	opt_f->_description = "Number of iterations in each kernel";
	opt_f->_type = streamsdk::CA_ARG_INT;
	opt_f->_value = &repeat;

	sampleArgs->AddOption(opt_f);
	delete opt_f;

	streamsdk::Option* opt_gws = new streamsdk::Option;
	CHECK_ALLOCATION(opt_gws, "Memory Allocation failed (opt_gws)");

	opt_gws->_sVersion = "";
	opt_gws->_lVersion = "GWS";
	opt_gws->_description = "Specify the GlobalWorkSize";
	opt_gws->_type = streamsdk::CA_ARG_INT;
	opt_gws->_value = &globalThreads;

	sampleArgs->AddOption(opt_gws);
	delete opt_gws;

	streamsdk::Option* opt_lws = new streamsdk::Option;
	CHECK_ALLOCATION(opt_lws, "Memory Allocation failed (opt_lws)");

	opt_lws->_sVersion = "";
	opt_lws->_lVersion = "LWS";
	opt_lws->_description = "Specify the localThreads";
	opt_lws->_type = streamsdk::CA_ARG_INT;
	opt_lws->_value = &localThreads;

	sampleArgs->AddOption(opt_lws);
	delete opt_lws;

	streamsdk::Option* opt_kernel = new streamsdk::Option;
	CHECK_ALLOCATION(opt_kernel, "Memory Allocation failed (opt_lws)");

	opt_kernel->_sVersion = "K";
	opt_kernel->_lVersion = "K";
	opt_kernel->_description = "Specify the kernel want to execute";
	opt_kernel->_type = streamsdk::CA_ARG_STRING;
	opt_kernel->_value = &kernelname;

	sampleArgs->AddOption(opt_kernel);
	delete opt_kernel;

	streamsdk::Option* opt_vector = new streamsdk::Option;
	CHECK_ALLOCATION(opt_vector, "Memory Allocation failed (opt_vector)");

	opt_vector->_sVersion = "";
	opt_vector->_lVersion = "v";
	opt_vector->_description = "Specify the kernel vectorsize want to execute[1|2|4|8|16]";
	opt_vector->_type = streamsdk::CA_ARG_INT;
	opt_vector->_value = &vectorSize;

	sampleArgs->AddOption(opt_vector);
	delete opt_vector;

	return SDK_SUCCESS;

}

int MathBenchmark::setupMemory(void) {
	unsigned int memSize = 64 * 1024 * 1024; //default
	length = memSize / sizeof(float);
	cl_uint size = length * sizeof(cl_float);

	// Allocate memory 
	input = (cl_float*) malloc(size);
	CHECK_ALLOCATION(input, "Failed to allocate host memory. (input)");

	// random initialisation of input
	sampleCommon->fillRandom<cl_float>(input, length, 1, 0, 32);

	return SDK_SUCCESS;

}

int MathBenchmark::setupCL(void) {
	cl_int status = 0;
	cl_device_type dType;

	if (deviceType.compare("cpu") == 0) {
		dType = CL_DEVICE_TYPE_CPU;
	} else //deviceType = "gpu"
	{
		dType = CL_DEVICE_TYPE_GPU;
		if (isThereGPU() == false) {
			std::cout << "GPU not found. Falling back to CPU device"
					<< std::endl;
			dType = CL_DEVICE_TYPE_CPU;
		}
	}

	/*
	 * Have a look at the available platforms and pick either
	 * the AMD one if available or a reasonable default.
	 */

	cl_platform_id platform = NULL;
	int retValue = sampleCommon->getPlatform(platform, platformId,
			isPlatformEnabled());
	CHECK_ERROR(retValue, SDK_SUCCESS, "sampleCommon::getPlatform() failed");

	// Display available devices.
	retValue = sampleCommon->displayDevices(platform, dType);
	CHECK_ERROR(retValue, SDK_SUCCESS, "sampleCommon::displayDevices() failed");

	/*
	 * If we could find our platform, use it. Otherwise use just available platform.
	 */

	cl_context_properties cps[3] = { CL_CONTEXT_PLATFORM,
			(cl_context_properties) platform, 0 };

	context = clCreateContextFromType(cps, dType, NULL, NULL, &status);
	CHECK_OPENCL_ERROR(status, "clCreateContextFromType failed.");
	
	// getting device on which to run the sample
	status = sampleCommon->getDevices(context, &devices, deviceId,
			isDeviceIdEnabled());
	CHECK_ERROR(status, SDK_SUCCESS, "sampleCommon::getDevices() failed");
	
	//Set device info of given cl_device_id
	retValue = deviceInfo.setDeviceInfo(devices[deviceId]);
	CHECK_ERROR(retValue, SDK_SUCCESS, "SDKDeviceInfo::setDeviceInfo() failed");

	maxWorkGroup = deviceInfo.maxWorkGroupSize;
	max_mem_alloc_size = deviceInfo.maxMemAllocSize;
	while (maxMemSize <= (unsigned int) (max_mem_alloc_size)) {
		maxMemSize *= 2;
	}
	maxMemSize /= 2;

	if (maxMemSize > 134217728 && dType == CL_DEVICE_TYPE_CPU) {
		maxMemSize = 134217728;
	}

	std::cout << "CL_DEVICE_MAX_WORK_GROUP_SIZE:\t" << maxWorkGroup
			<< std::endl;
	std::cout << "MaxMemSize:\t" << maxMemSize / (1024 * 1024) << "MB"
			<< std::endl;
	{
		// The block is to move the declaration of prop closer to its use
		cl_command_queue_properties prop = 0;
		prop |= CL_QUEUE_PROFILING_ENABLE;
		commandQueue = clCreateCommandQueue(context, devices[deviceId], prop,
				&status);
		CHECK_OPENCL_ERROR(status, "clCreateCommandQueue failed.");
	}
	// create a CL program using the kernel source
	streamsdk::buildProgramData buildData;
	buildData.kernelName = std::string("mathoper.cl");
	buildData.devices = devices;
	buildData.deviceId = deviceId;
	buildData.flagsStr = std::string("");
	if (isLoadBinaryEnabled())
		buildData.binaryName = std::string(loadBinary.c_str());

	if (isComplierFlagsSpecified())
		buildData.flagsFileName = std::string(flags.c_str());

	retValue = sampleCommon->buildOpenCLProgram(program, context, buildData);
	CHECK_ERROR(retValue, SDK_SUCCESS,
			"sampleCommon::buildOpenCLProgram() failed");
	
	
    	std::string s;
	std::stringstream powr_withDD(s);
	powr_withDD << "kernel_powr_withDD";
	powr_withDD << vectorSize;
	//dumpPTXCode(context,program,powr_withDD.str().c_str());
	// Create the cKermel_kernel_powr_withDD
	kernel[0]  = clCreateKernel(program, powr_withDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_powr_withDD)");

	std::stringstream powr_withoutDD(s);
	powr_withoutDD << "kernel_powr_withoutDD";
	powr_withoutDD << vectorSize;
	//dumpPTXCode(context,program,powr_withoutDD.str().c_str());
	// Create the cKermel_kernel_powr_withoutDD
	kernel[1]  = clCreateKernel(program, powr_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_powr_withoutDD)");

	std::stringstream remquo_withDD(s);
	remquo_withDD << "kernel_remquo_withDD";
	remquo_withDD << vectorSize;
	//dumpPTXCode(context,program,remquo_withDD.str().c_str());
	// Create the cKermel_kernel_remquo_withDD
	kernel[2]  = clCreateKernel(program, remquo_withDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_remquo_withDD)");

	std::stringstream remquo_withoutDD(s);
	remquo_withoutDD << "kernel_remquo_withoutDD";
	remquo_withoutDD << vectorSize;
	//dumpPTXCode(context,program,remquo_withoutDD.str().c_str());
	// Create the cKermel_kernel_remquo_withoutDD
	kernel[3]  = clCreateKernel(program, remquo_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_remquo_withoutDD)");

	std::stringstream rootn_withDD(s);
	rootn_withDD << "kernel_rootn_withDD";
	rootn_withDD << vectorSize;
	//dumpPTXCode(context,program,rootn_withDD.str().c_str());
	// Create the cKermel_kernel_rootn_withDD
	kernel[4]  = clCreateKernel(program, rootn_withDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_rootn_withDD)");

	std::stringstream rootn_withoutDD(s);
	rootn_withoutDD << "kernel_rootn_withoutDD";
	rootn_withoutDD << vectorSize;
	//dumpPTXCode(context,program,rootn_withoutDD.str().c_str());
	// Create the cKermel_kernel_rootn_withoutDD
	kernel[5]  = clCreateKernel(program, rootn_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_rootn_withoutDD)");


	std::stringstream tgamma_withDD(s);
	tgamma_withDD << "kernel_tgamma_withDD";
	tgamma_withDD << vectorSize;
	//dumpPTXCode(context,program,tgamma_withDD.str().c_str());
	// Create the cKermel_kernel_tgamma_withDD
	kernel[6]  = clCreateKernel(program, tgamma_withDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_tgamma_withDD)");

	std::stringstream tgamma_withoutDD(s);
	tgamma_withoutDD << "kernel_tgamma_withoutDD";
	tgamma_withoutDD << vectorSize;
	//dumpPTXCode(context,program,tgamma_withoutDD.str().c_str());
	// Create the cKermel_kernel_tgamma_withoutDD
	kernel[7]  = clCreateKernel(program, tgamma_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_tgamma_withoutDD)");

	return SDK_SUCCESS;
}

int MathBenchmark::setup() {
	//if (setupMemory() != SDK_SUCCESS)
	//	return SDK_FAILURE;

	int timer = sampleCommon->createTimer();
	sampleCommon->resetTimer(timer);
	sampleCommon->startTimer(timer);

	int status = setupCL();
	if (status != SDK_SUCCESS) {
		if (status == SDK_EXPECTED_FAILURE)
			return SDK_EXPECTED_FAILURE;

		return SDK_FAILURE;
	}

	sampleCommon->stopTimer(timer);
	setupTime = (cl_double) sampleCommon->readTimer(timer);
	return SDK_SUCCESS;
}

double MathBenchmark::executionTime(cl_event &event) {
	cl_ulong start, end;
	cl_int status = 0;

	status = clGetEventProfilingInfo(event, CL_PROFILING_COMMAND_END,
			sizeof(cl_ulong), &end, NULL);
	status |= clGetEventProfilingInfo(event, CL_PROFILING_COMMAND_START,
			sizeof(cl_ulong), &start, NULL);

	CHECK_OPENCL_ERROR(status, "clGetEventProfilingInfo failed. (event)");

	return (double) 1.0e-6 * (end - start); // convert nanoseconds to min seconds on return
}

int MathBenchmark::runCLKernels() {
	cl_int status = 0;
	size_t WorkDim = 1;
	size_t szGlobalWorkSize = globalThreads;
	size_t szLocalWorkSize = localThreads;
	size_t szForNum = repeat;

	//create event to record time
	cl_event event_powr_withDD, event_powr_withoutDD;
	cl_event event_remquo_withDD, event_remquo_withoutDD;
	cl_event event_rootn_withDD, event_rootn_withoutDD;
	cl_event event_tgamma_withDD, event_tgamma_withoutDD;

	//Create Variable for story result reading from device to host
	void *powr_withDD_num, *powr_withoutDD_num;
	void *remquo_withDD_num, *remquo_withoutDD_num;
	void *rootn_withDD_num, *rootn_withoutDD_num;
	void *tgamma_withDD_num, *tgamma_withoutDD_num;

	powr_withDD_num = (void *) malloc(sizeof(cl_float));
	powr_withoutDD_num = (void *) malloc(sizeof(cl_float));
	remquo_withDD_num = (void *) malloc(sizeof(cl_float));
	remquo_withoutDD_num = (void *) malloc(sizeof(cl_float));
	rootn_withDD_num = (void *) malloc(sizeof(cl_float));
	rootn_withoutDD_num = (void *) malloc(sizeof(cl_float));
	tgamma_withDD_num = (void *) malloc(sizeof(cl_float));
	tgamma_withoutDD_num = (void *) malloc(sizeof(cl_float));

	float powr_withDD_maxGflops = 0.0;
	float powr_withoutDD_maxGflops = 0.0;
	float remquo_withDD_maxGflops = 0.0;
	float remquo_withoutDD_maxGflops = 0.0;
	float rootn_withDD_maxGflops = 0.0;
	float rootn_withoutDD_maxGflops = 0.0;
	float tgamma_withDD_maxGflops = 0.0;
	float tgamma_withoutDD_maxGflops = 0.0;

	//create buffer

	cl_mem result_powr_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_powr_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_remquo_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_remquo_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_rootn_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_rootn_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_tgamma_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_tgamma_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);

	//set kernel_powr_withDD Argument
	status |= clSetKernelArg(kernel[0] , 0, sizeof(cl_mem),
			(void*) &result_powr_withDD);
	status |= clSetKernelArg(kernel[0] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_powr_withDD)");
	
	//set kernel_powr_withoutDD Argument
	status |= clSetKernelArg(kernel[1] , 0, sizeof(cl_mem),
			(void*) &result_powr_withoutDD);
	status |= clSetKernelArg(kernel[1] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_powr_withoutDD)");

	//set kernel_remquo_withDD Argument
	status |= clSetKernelArg(kernel[2] , 0, sizeof(cl_mem),
			(void*) &result_remquo_withDD);
	status |= clSetKernelArg(kernel[2] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_remquo_withDD)");

	//set kernel_remquo_withoutDD Argument
	status |= clSetKernelArg(kernel[3] , 0, sizeof(cl_mem),
			(void*) &result_remquo_withoutDD);
	status |= clSetKernelArg(kernel[3] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_remquo_withoutDD)");

	//set kernel_rootn_withDD Argument
	status |= clSetKernelArg(kernel[4] , 0, sizeof(cl_mem),
			(void*) &result_rootn_withDD);
	status |= clSetKernelArg(kernel[4] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_rootn_withDD)");

	//set kernel_rootn_withoutDD Argument
	status |= clSetKernelArg(kernel[5] , 0, sizeof(cl_mem),
			(void*) &result_rootn_withoutDD);
	status |= clSetKernelArg(kernel[5] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_rootn_withoutDD)");


	//set kernel_tgamma_withDD Argument
	status |= clSetKernelArg(kernel[6] , 0, sizeof(cl_mem),
			(void*) &result_tgamma_withDD);
	status |= clSetKernelArg(kernel[6] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_tgamma_withDD)");

	//set kernel_tgamma_withoutDD Argument
	status |= clSetKernelArg(kernel[7] , 0, sizeof(cl_mem),
			(void*) &result_tgamma_withoutDD);
	status |= clSetKernelArg(kernel[7] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_tgamma_withoutDD)");



	int i = 0;
	int gws, lws;
	for (gws = 1024; gws <= 65536 * 8; gws *= 2)
		for (lws = 64; lws <= 256; lws *= 2) {
			
			float executionTime_powr_withDD_max = 0.0;
			float executionTime_powr_withDD_avg = 0.0;
			float executionTime_powr_withDD_min = 999999999.0;

			float executionTime_powr_withoutDD_max = 0.0;
			float executionTime_powr_withoutDD_avg = 0.0;
			float executionTime_powr_withoutDD_min = 999999999.0;

			float executionTime_remquo_withDD_max = 0.0;
			float executionTime_remquo_withDD_avg = 0.0;
			float executionTime_remquo_withDD_min = 999999999.0;

			float executionTime_remquo_withoutDD_max = 0.0;
			float executionTime_remquo_withoutDD_avg = 0.0;
			float executionTime_remquo_withoutDD_min = 999999999.0;

			float executionTime_rootn_withDD_max = 0.0;
			float executionTime_rootn_withDD_avg = 0.0;
			float executionTime_rootn_withDD_min = 999999999.0;

			float executionTime_rootn_withoutDD_max = 0.0;
			float executionTime_rootn_withoutDD_avg = 0.0;
			float executionTime_rootn_withoutDD_min = 999999999.0;

			float executionTime_tgamma_withDD_max = 0.0;
			float executionTime_tgamma_withDD_avg = 0.0;
			float executionTime_tgamma_withDD_min = 999999999.0;

			float executionTime_tgamma_withoutDD_max = 0.0;
			float executionTime_tgamma_withoutDD_avg = 0.0;
			float executionTime_tgamma_withoutDD_min = 999999999.0;

			szGlobalWorkSize = gws;
			szLocalWorkSize = lws;
			printf("-----------------------------------------------------\n");
			printf("Set gws = %d , lws = %d\n", gws, lws);
			
			//launch kernel_powr_withDD
			if (!strcmp(kernelname.c_str(), "powr_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_powr_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[0] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_powr_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_powr
					status = clWaitForEvents(1, &event_powr_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_powr_withDD, end_powr_withDD;
					status = clGetEventProfilingInfo(event_powr_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_powr_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_powr_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_powr_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_powr_withDD = (end_powr_withDD
							- start_powr_withDD);
					if (executionTime_powr_withDD_max
							< executionTime_powr_withDD) {
						executionTime_powr_withDD_max = executionTime_powr_withDD;
					}
					if (executionTime_powr_withDD_min
							> executionTime_powr_withDD) {
						executionTime_powr_withDD_min = executionTime_powr_withDD;
					}
					executionTime_powr_withDD_avg += executionTime_powr_withDD;

				}
				executionTime_powr_withDD_avg = (executionTime_powr_withDD_avg
						- executionTime_powr_withDD_max
						- executionTime_powr_withDD_min) / (iterations - 2);
				size_t time_powr_withDD=0;
					time_powr_withDD= 64*szForNum * vectorSize;
				float Gflops_powr_withDD =
						(time_powr_withDD * szGlobalWorkSize)
								/ executionTime_powr_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_powr_withDD,
						CL_TRUE, 0, sizeof(cl_mem), powr_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-powr_withDD : %f\n", Gflops_powr_withDD);
				printf("Result-powr_withDD   :   %f\n\n", *((float*)powr_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_powr_withDD > powr_withDD_maxGflops){
						powr_withDD_maxGflops = Gflops_powr_withDD;
				}
			}

			//launch kernel_powr_withoutDD
			if (!strcmp(kernelname.c_str(), "powr_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_powr_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[1] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_powr_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_powr
					status = clWaitForEvents(1, &event_powr_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_powr_withoutDD, end_powr_withoutDD;
					status = clGetEventProfilingInfo(event_powr_withoutDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_powr_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_powr_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_powr_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_powr_withoutDD = (end_powr_withoutDD
							- start_powr_withoutDD);
					if (executionTime_powr_withoutDD_max
							< executionTime_powr_withoutDD) {
						executionTime_powr_withoutDD_max = executionTime_powr_withoutDD;
					}
					if (executionTime_powr_withoutDD_min
							> executionTime_powr_withoutDD) {
						executionTime_powr_withoutDD_min = executionTime_powr_withoutDD;
					}
					executionTime_powr_withoutDD_avg += executionTime_powr_withoutDD;

				}
				executionTime_powr_withoutDD_avg = (executionTime_powr_withoutDD_avg
						- executionTime_powr_withoutDD_max
						- executionTime_powr_withoutDD_min) / (iterations - 2);
				size_t time_powr_withoutDD=0;
				time_powr_withoutDD=  7 * 10  *szForNum * vectorSize;
				float Gflops_powr_withoutDD =
						(time_powr_withoutDD * szGlobalWorkSize)
								/ executionTime_powr_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_powr_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), powr_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-powr_withoutDD : %f\n", Gflops_powr_withoutDD);
				printf("Result-powr_withoutDD   :   %f\n\n", *((float*)powr_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_powr_withoutDD > powr_withoutDD_maxGflops){
						powr_withoutDD_maxGflops = Gflops_powr_withoutDD;
				}

			}

			//launch kernel_remquo_withDD
			if (!strcmp(kernelname.c_str(), "remquo_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_remquo_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[2] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_remquo_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_remquo
					status = clWaitForEvents(1, &event_remquo_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
