							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_remquo_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_remquo_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_remquo_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_remquo_withoutDD = (end_remquo_withoutDD
							- start_remquo_withoutDD);
					if (executionTime_remquo_withoutDD_max
							< executionTime_remquo_withoutDD) {
						executionTime_remquo_withoutDD_max = executionTime_remquo_withoutDD;
					}
					if (executionTime_remquo_withoutDD_min
							> executionTime_remquo_withoutDD) {
						executionTime_remquo_withoutDD_min = executionTime_remquo_withoutDD;
					}
					executionTime_remquo_withoutDD_avg += executionTime_remquo_withoutDD;

				}
				executionTime_remquo_withoutDD_avg = (executionTime_remquo_withoutDD_avg
						- executionTime_remquo_withoutDD_max
						- executionTime_remquo_withoutDD_min) / (iterations - 2);
				size_t time_remquo_withoutDD=0;
				time_remquo_withoutDD= 7 * szForNum * vectorSize;
				float Gflops_remquo_withoutDD =
						(time_remquo_withoutDD * szGlobalWorkSize)
								/ executionTime_remquo_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_remquo_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), remquo_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-remquo_withoutDD : %f\n", Gflops_remquo_withoutDD);
				printf("Result-remquo_withoutDD   :   %f\n\n", *((float*)remquo_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_remquo_withoutDD > remquo_withoutDD_maxGflops){
						remquo_withoutDD_maxGflops = Gflops_remquo_withoutDD;
				}
			}

			//launch kernel_rootn_withDD
			if (!strcmp(kernelname.c_str(), "rootn_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_rootn_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[4] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_rootn_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_rootn
					status = clWaitForEvents(1, &event_rootn_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_rootn_withDD, end_rootn_withDD;
					status = clGetEventProfilingInfo(event_rootn_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_rootn_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_rootn_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_rootn_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_rootn_withDD = (end_rootn_withDD
							- start_rootn_withDD);
					if (executionTime_rootn_withDD_max
							< executionTime_rootn_withDD) {
						executionTime_rootn_withDD_max = executionTime_rootn_withDD;
					}
					if (executionTime_rootn_withDD_min
							> executionTime_rootn_withDD) {
						executionTime_rootn_withDD_min = executionTime_rootn_withDD;
					}
					executionTime_rootn_withDD_avg += executionTime_rootn_withDD;

				}
				executionTime_rootn_withDD_avg = (executionTime_rootn_withDD_avg
						- executionTime_rootn_withDD_max
						- executionTime_rootn_withDD_min) / (iterations - 2);
				size_t time_rootn_withDD=0;
					time_rootn_withDD= 64 * szForNum * vectorSize;
				float Gflops_rootn_withDD =
						(time_rootn_withDD * szGlobalWorkSize)
								/ executionTime_rootn_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_rootn_withDD,
						CL_TRUE, 0, sizeof(cl_mem), rootn_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-rootn_withDD : %f\n", Gflops_rootn_withDD);
				printf("Result-rootn_withDD   :   %f\n\n", *((float*)rootn_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_rootn_withDD > rootn_withDD_maxGflops){
						rootn_withDD_maxGflops = Gflops_rootn_withDD;
				}
			}

			//launch kernel_rootn_withoutDD
			if (!strcmp(kernelname.c_str(), "rootn_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_rootn_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[5] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_rootn_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_rootn
					status = clWaitForEvents(1, &event_rootn_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_rootn_withoutDD, end_rootn_withoutDD;
					status = clGetEventProfilingInfo(event_rootn_withoutDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_rootn_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_rootn_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_rootn_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_rootn_withoutDD = (end_rootn_withoutDD
							- start_rootn_withoutDD);
					if (executionTime_rootn_withoutDD_max
							< executionTime_rootn_withoutDD) {
						executionTime_rootn_withoutDD_max = executionTime_rootn_withoutDD;
					}
					if (executionTime_rootn_withoutDD_min
							> executionTime_rootn_withoutDD) {
						executionTime_rootn_withoutDD_min = executionTime_rootn_withoutDD;
					}
					executionTime_rootn_withoutDD_avg += executionTime_rootn_withoutDD;

				}
				executionTime_rootn_withoutDD_avg = (executionTime_rootn_withoutDD_avg
						- executionTime_rootn_withoutDD_max
						- executionTime_rootn_withoutDD_min) / (iterations - 2);
				size_t time_rootn_withoutDD=0;
				time_rootn_withoutDD= 7 * 10* szForNum * vectorSize;
				float Gflops_rootn_withoutDD =
						(time_rootn_withoutDD * szGlobalWorkSize)
								/ executionTime_rootn_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_rootn_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), rootn_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-rootn_withoutDD : %f\n", Gflops_rootn_withoutDD);
				printf("Result-rootn_withoutDD   :   %f\n\n", *((float*)rootn_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_rootn_withoutDD > rootn_withoutDD_maxGflops){
						rootn_withoutDD_maxGflops = Gflops_rootn_withoutDD;
				}

			}

			//launch kernel_tgamma_withDD
			if (!strcmp(kernelname.c_str(), "tgamma_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_tgamma_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[6] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_tgamma_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_tgamma
					status = clWaitForEvents(1, &event_tgamma_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_tgamma_withDD, end_tgamma_withDD;
					status = clGetEventProfilingInfo(event_tgamma_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_tgamma_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_tgamma_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_tgamma_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_tgamma_withDD = (end_tgamma_withDD
							- start_tgamma_withDD);
					if (executionTime_tgamma_withDD_max
							< executionTime_tgamma_withDD) {
						executionTime_tgamma_withDD_max = executionTime_tgamma_withDD;
					}
					if (executionTime_tgamma_withDD_min
							> executionTime_tgamma_withDD) {
						executionTime_tgamma_withDD_min = executionTime_tgamma_withDD;
					}
					executionTime_tgamma_withDD_avg += executionTime_tgamma_withDD;

				}
				executionTime_tgamma_withDD_avg = (executionTime_tgamma_withDD_avg
						- executionTime_tgamma_withDD_max
						- executionTime_tgamma_withDD_min) / (iterations - 2);
				size_t time_tgamma_withDD=0;
					time_tgamma_withDD= 64*szForNum * vectorSize;
				float Gflops_tgamma_withDD =
						(time_tgamma_withDD * szGlobalWorkSize)
								/ executionTime_tgamma_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_tgamma_withDD,
						CL_TRUE, 0, sizeof(cl_mem), tgamma_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-tgamma_withDD : %f\n", Gflops_tgamma_withDD);
				printf("Result-tgamma_withDD   :   %f\n\n", *((float*)tgamma_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_tgamma_withDD > tgamma_withDD_maxGflops){
						tgamma_withDD_maxGflops = Gflops_tgamma_withDD;
				}
			}

			//launch kernel_tgamma_withoutDD
			if (!strcmp(kernelname.c_str(), "tgamma_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_tgamma_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[7] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_tgamma_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_tgamma
					status = clWaitForEvents(1, &event_tgamma_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_tgamma_withoutDD, end_tgamma_withoutDD;
					status = clGetEventProfilingInfo(event_tgamma_withoutDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_tgamma_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_tgamma_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_tgamma_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_tgamma_withoutDD = (end_tgamma_withoutDD
							- start_tgamma_withoutDD);
					if (executionTime_tgamma_withoutDD_max
							< executionTime_tgamma_withoutDD) {
						executionTime_tgamma_withoutDD_max = executionTime_tgamma_withoutDD;
					}
					if (executionTime_tgamma_withoutDD_min
							> executionTime_tgamma_withoutDD) {
						executionTime_tgamma_withoutDD_min = executionTime_tgamma_withoutDD;
					}
					executionTime_tgamma_withoutDD_avg += executionTime_tgamma_withoutDD;

				}
				executionTime_tgamma_withoutDD_avg = (executionTime_tgamma_withoutDD_avg
						- executionTime_tgamma_withoutDD_max
						- executionTime_tgamma_withoutDD_min) / (iterations - 2);
				size_t time_tgamma_withoutDD=0;
				time_tgamma_withoutDD= 7 * 10 * szForNum * vectorSize;
				float Gflops_tgamma_withoutDD =
						(time_tgamma_withoutDD * szGlobalWorkSize)
								/ executionTime_tgamma_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_tgamma_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), tgamma_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-tgamma_withoutDD : %f\n", Gflops_tgamma_withoutDD);
				printf("Result-tgamma_withoutDD   :   %f\n\n", *((float*)tgamma_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_tgamma_withoutDD > tgamma_withoutDD_maxGflops){
						tgamma_withoutDD_maxGflops = Gflops_tgamma_withoutDD;
				}

			}

	}	
	printf("powr_withDD_maxGflops = %f\n",powr_withDD_maxGflops);
	printf("powr_withoutDD_maxGflops = %f\n",powr_withoutDD_maxGflops);
	printf("remquo_withDD_maxGflops = %f\n",remquo_withDD_maxGflops);
	printf("remquo_withoutDD_maxGflops = %f\n",remquo_withoutDD_maxGflops);
	printf("rootn_withDD_maxGflops = %f\n",rootn_withDD_maxGflops);
	printf("rootn_withoutDD_maxGflops = %f\n",rootn_withoutDD_maxGflops);
	printf("tgamma_withDD_maxGflops = %f\n",tgamma_withDD_maxGflops);
	printf("tgamma_withoutDD_maxGflops = %f\n",tgamma_withoutDD_maxGflops);

	return SDK_SUCCESS;
}

int MathBenchmark::run() {
/*	
	 // Warm up
	 for(int i = 0; i < 2 && iterations != 1; i++)
	 {
	 // Arguments are set and execution call is enqueued on command buffer
	 if(runCLKernels() != SDK_SUCCESS)
	 return SDK_FAILURE;
	 }
*/	
	int timer = sampleCommon->createTimer();
	sampleCommon->resetTimer(timer);
	sampleCommon->startTimer(timer);
	if (runCLKernels() != SDK_SUCCESS)
		return SDK_FAILURE;
	sampleCommon->stopTimer(timer);
	totalKernelTime = (double) sampleCommon->readTimer(timer);

	//if(!quiet) {
	//	sampleCommon->printArray<cl_float>("Output", eigenIntervals[1], 2*length, 1);
	//}

	return SDK_SUCCESS;
}

int MathBenchmark::cleanup() {
	// Releases OpenCL resources (Context, Memory etc.)
	cl_int status;

	for (int i = 0; i < NUM_KERNELS; i++) {
		status = clReleaseKernel(kernel[i]);
		CHECK_OPENCL_ERROR(status, "clReleaseKernel failed.");
	}

	status = clReleaseProgram(program);
	CHECK_OPENCL_ERROR(status, "clReleaseProgram failed.(program)");

	status = clReleaseCommandQueue(commandQueue);
	CHECK_OPENCL_ERROR(status, "clReleaseCommandQueue failed.(commandQueue)");

	status = clReleaseContext(context);
	CHECK_OPENCL_ERROR(status, "clReleaseContext failed.(context)");

	FREE(devices);
	return SDK_SUCCESS;
}

int MathBenchmark::verifyResults() {
	return SDK_SUCCESS;
}

void MathBenchmark::printStats() {
	std::string strArray[3] = { "DiagonalLength", "Time(sec)",
			"[Transfer+Kernel]Time(sec)" };
	std::string stats[3];

	totalTime = setupTime + totalKernelTime;

	stats[0] = sampleCommon->toString(1410, std::dec);
	stats[1] = sampleCommon->toString(totalTime, std::dec);
	stats[2] = sampleCommon->toString(totalKernelTime, std::dec);

	this->Benchmark::printStats(strArray, stats, 3);
}

int main(int argc, char *argv[]) {
	cl_int status = 0;

	MathBenchmark clMath("Math1 benchmark");

	if (clMath.initialize() != SDK_SUCCESS)
		return SDK_FAILURE;

	if (clMath.parseCommandLine(argc, argv) != SDK_SUCCESS)
		return SDK_FAILURE;

	status = clMath.setup();
	if (status != SDK_SUCCESS) {
		if (status == SDK_EXPECTED_FAILURE)
			return SDK_SUCCESS;

		return SDK_FAILURE;
	}

	if (clMath.run() != SDK_SUCCESS)
		return SDK_FAILURE;

	if (clMath.verifyResults() != SDK_SUCCESS)
		return SDK_FAILURE;

	if (clMath.cleanup() != SDK_SUCCESS)
		return SDK_FAILURE;

	clMath.printStats();

	return SDK_SUCCESS;
}


