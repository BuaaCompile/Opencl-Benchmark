#include "MathBasic.hpp"

// ****************************************************************************
// Method:  oclGetProgBinary
//
// Purpose:
//   Get the binary (PTX) of the program associated with the device
//
// Arguments:
//       cpProgram    OpenCL program
//       cdDevice     device of interest
//       binary       returned code
//       length       length of returned code
//
// Copyright 1993-2013 NVIDIA Corporation
//
// ****************************************************************************
inline void
oclGetProgBinary (cl_program cpProgram, cl_device_id cdDevice, char** binary, size_t* length)
{
    // Grab the number of devices associated witht the program
    cl_uint num_devices;
    clGetProgramInfo(cpProgram, CL_PROGRAM_NUM_DEVICES, sizeof(cl_uint), &num_devices, NULL);

    // Grab the device ids
    cl_device_id* devices = (cl_device_id*) malloc(num_devices * sizeof(cl_device_id));
    clGetProgramInfo(cpProgram, CL_PROGRAM_DEVICES, num_devices * sizeof(cl_device_id), devices, 0);

    // Grab the sizes of the binaries
    size_t* binary_sizes = (size_t*)malloc(num_devices * sizeof(size_t));
    clGetProgramInfo(cpProgram, CL_PROGRAM_BINARY_SIZES, num_devices * sizeof(size_t), binary_sizes, NULL);

    // Now get the binaries
    char** ptx_code = (char**) malloc(num_devices * sizeof(char*));
    for( unsigned int i=0; i<num_devices; ++i) {
        ptx_code[i]= (char*)malloc(binary_sizes[i]);
    }
    clGetProgramInfo(cpProgram, CL_PROGRAM_BINARIES, 0, ptx_code, NULL);

    // Find the index of the device of interest
    unsigned int idx = 0;
    while( idx<num_devices && devices[idx] != cdDevice ) ++idx;

    // If it is associated prepare the result
    if( idx < num_devices )
    {
        *binary = ptx_code[idx];
        *length = binary_sizes[idx];
    }

    // Cleanup
    free( devices );
    free( binary_sizes );
    for( unsigned int i=0; i<num_devices; ++i) {
        if( i != idx ) free(ptx_code[i]);
    }
    free( ptx_code );
}

// ****************************************************************************
// Method:  oclGetFirstDev
//
// Purpose:
//   Gets the id of the first device from the context
//
// Arguments:
//       cxMainContext         OpenCL context
//
// Copyright 1993-2013 NVIDIA Corporation
//
// ****************************************************************************
inline cl_device_id
oclGetFirstDev(cl_context cxMainContext)
{
    size_t szParmDataBytes;
    cl_device_id* cdDevices;

    // get the list of GPU devices associated with context
    clGetContextInfo(cxMainContext, CL_CONTEXT_DEVICES, 0, NULL, &szParmDataBytes);
    cdDevices = (cl_device_id*) malloc(szParmDataBytes);

    clGetContextInfo(cxMainContext, CL_CONTEXT_DEVICES, szParmDataBytes, cdDevices, NULL);

    cl_device_id first = cdDevices[0];
    free(cdDevices);

    return first;
}


// ****************************************************************************
// Method:  dumpPTXCode
//
// Purpose:
//
//
// Arguments:
//   ctx          context
//   prg          program
//   name         file name prefix to output to
//
// Programmer:  Gabriel Marin
// Creation:    July 14, 2009
//
// ****************************************************************************
inline bool
dumpPTXCode (cl_context ctx, cl_program prog, const char *name)
{
    std::cout << "Dumping the PTX code" << std::endl;
    size_t ptx_length;
    char* ptx_code;
    char buf[64];
    oclGetProgBinary (prog, oclGetFirstDev(ctx), &ptx_code, &ptx_length);

    FILE* ptxFile = NULL;
    sprintf (buf, "%.59s.ptx", name);
#ifdef WIN32
    fopen_s (&ptxFile, buf, "w");
#else
    ptxFile = fopen (buf,"w");
#endif
    if (ptxFile)
    {
        fwrite (ptx_code, ptx_length, 1, ptxFile);
        fclose (ptxFile);
    }
    free (ptx_code);
    return (ptx_code!=0);
}



int MathBenchmark::initialize() {
	// Call base class Initialize to get default configuration
	if (this->Benchmark::initialize() != SDK_SUCCESS)
		return SDK_FAILURE;

	streamsdk::Option* opt_n = new streamsdk::Option;
	CHECK_ALLOCATION(opt_n, "Memory Allocation failed (opt_n)");

	opt_n->_sVersion = "N";
	opt_n->_lVersion = "N";
	opt_n->_description = "Number of iterations for kernel execution";
	opt_n->_type = streamsdk::CA_ARG_INT;
	opt_n->_value = &iterations;

	sampleArgs->AddOption(opt_n);
	delete opt_n;

	streamsdk::Option* opt_f = new streamsdk::Option;
	CHECK_ALLOCATION(opt_f, "Memory Allocation failed (opt_f)");

	opt_f->_sVersion = "F";
	opt_f->_lVersion = "F";
	opt_f->_description = "Number of iterations in each kernel";
	opt_f->_type = streamsdk::CA_ARG_INT;
	opt_f->_value = &repeat;

	sampleArgs->AddOption(opt_f);
	delete opt_f;

	streamsdk::Option* opt_gws = new streamsdk::Option;
	CHECK_ALLOCATION(opt_gws, "Memory Allocation failed (opt_gws)");

	opt_gws->_sVersion = "";
	opt_gws->_lVersion = "GWS";
	opt_gws->_description = "Specify the GlobalWorkSize";
	opt_gws->_type = streamsdk::CA_ARG_INT;
	opt_gws->_value = &globalThreads;

	sampleArgs->AddOption(opt_gws);
	delete opt_gws;

	streamsdk::Option* opt_lws = new streamsdk::Option;
	CHECK_ALLOCATION(opt_lws, "Memory Allocation failed (opt_lws)");

	opt_lws->_sVersion = "";
	opt_lws->_lVersion = "LWS";
	opt_lws->_description = "Specify the localThreads";
	opt_lws->_type = streamsdk::CA_ARG_INT;
	opt_lws->_value = &localThreads;

	sampleArgs->AddOption(opt_lws);
	delete opt_lws;

	streamsdk::Option* opt_kernel = new streamsdk::Option;
	CHECK_ALLOCATION(opt_kernel, "Memory Allocation failed (opt_lws)");

	opt_kernel->_sVersion = "K";
	opt_kernel->_lVersion = "K";
	opt_kernel->_description = "Specify the kernel want to execute";
	opt_kernel->_type = streamsdk::CA_ARG_STRING;
	opt_kernel->_value = &kernelname;

	sampleArgs->AddOption(opt_kernel);
	delete opt_kernel;

	streamsdk::Option* opt_vector = new streamsdk::Option;
	CHECK_ALLOCATION(opt_vector, "Memory Allocation failed (opt_vector)");

	opt_vector->_sVersion = "";
	opt_vector->_lVersion = "v";
	opt_vector->_description = "Specify the kernel vectorsize want to execute[1|2|4|8|16]";
	opt_vector->_type = streamsdk::CA_ARG_INT;
	opt_vector->_value = &vectorSize;

	sampleArgs->AddOption(opt_vector);
	delete opt_vector;

	return SDK_SUCCESS;

}

int MathBenchmark::setupMemory(void) {
	unsigned int memSize = 64 * 1024 * 1024; //default
	length = memSize / sizeof(float);
	cl_uint size = length * sizeof(cl_float);

	// Allocate memory 
	input = (cl_float*) malloc(size);
	CHECK_ALLOCATION(input, "Failed to allocate host memory. (input)");

	// random initialisation of input
	sampleCommon->fillRandom<cl_float>(input, length, 1, 0, 32);

	return SDK_SUCCESS;

}

int MathBenchmark::setupCL(void) {
	cl_int status = 0;
	cl_device_type dType;

	if (deviceType.compare("cpu") == 0) {
		dType = CL_DEVICE_TYPE_CPU;
	} else //deviceType = "gpu"
	{
		dType = CL_DEVICE_TYPE_GPU;
		if (isThereGPU() == false) {
			std::cout << "GPU not found. Falling back to CPU device"
					<< std::endl;
			dType = CL_DEVICE_TYPE_CPU;
		}
	}

	/*
	 * Have a look at the available platforms and pick either
	 * the AMD one if available or a reasonable default.
	 */

	cl_platform_id platform = NULL;
	int retValue = sampleCommon->getPlatform(platform, platformId,
			isPlatformEnabled());
	CHECK_ERROR(retValue, SDK_SUCCESS, "sampleCommon::getPlatform() failed");

	// Display available devices.
	retValue = sampleCommon->displayDevices(platform, dType);
	CHECK_ERROR(retValue, SDK_SUCCESS, "sampleCommon::displayDevices() failed");

	/*
	 * If we could find our platform, use it. Otherwise use just available platform.
	 */

	cl_context_properties cps[3] = { CL_CONTEXT_PLATFORM,
			(cl_context_properties) platform, 0 };

	context = clCreateContextFromType(cps, dType, NULL, NULL, &status);
	CHECK_OPENCL_ERROR(status, "clCreateContextFromType failed.");
	
	// getting device on which to run the sample
	status = sampleCommon->getDevices(context, &devices, deviceId,
			isDeviceIdEnabled());
	CHECK_ERROR(status, SDK_SUCCESS, "sampleCommon::getDevices() failed");
	
	//Set device info of given cl_device_id
	retValue = deviceInfo.setDeviceInfo(devices[deviceId]);
	CHECK_ERROR(retValue, SDK_SUCCESS, "SDKDeviceInfo::setDeviceInfo() failed");

	maxWorkGroup = deviceInfo.maxWorkGroupSize;
	max_mem_alloc_size = deviceInfo.maxMemAllocSize;
	while (maxMemSize <= (unsigned int) (max_mem_alloc_size)) {
		maxMemSize *= 2;
	}
	maxMemSize /= 2;

	if (maxMemSize > 134217728 && dType == CL_DEVICE_TYPE_CPU) {
		maxMemSize = 134217728;
	}

	std::cout << "CL_DEVICE_MAX_WORK_GROUP_SIZE:\t" << maxWorkGroup
			<< std::endl;
	std::cout << "MaxMemSize:\t" << maxMemSize / (1024 * 1024) << "MB"
			<< std::endl;
	{
		// The block is to move the declaration of prop closer to its use
		cl_command_queue_properties prop = 0;
		prop |= CL_QUEUE_PROFILING_ENABLE;
		commandQueue = clCreateCommandQueue(context, devices[deviceId], prop,
				&status);
		CHECK_OPENCL_ERROR(status, "clCreateCommandQueue failed.");
	}
	// create a CL program using the kernel source
	streamsdk::buildProgramData buildData;
	buildData.kernelName = std::string("mathoper.cl");
	buildData.devices = devices;
	buildData.deviceId = deviceId;
	buildData.flagsStr = std::string("");
	if (isLoadBinaryEnabled())
		buildData.binaryName = std::string(loadBinary.c_str());

	if (isComplierFlagsSpecified())
		buildData.flagsFileName = std::string(flags.c_str());

	retValue = sampleCommon->buildOpenCLProgram(program, context, buildData);
	CHECK_ERROR(retValue, SDK_SUCCESS,
			"sampleCommon::buildOpenCLProgram() failed");
	
	
    	std::string s;
   	std::stringstream ss(s);
    	ss << "kernel_sin_withDD";
   	ss << vectorSize;
	// Create the cKermel_kernel_sin_withDD
	kernel[0]  = clCreateKernel(program, ss.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_sin_withDD)");
	
	std::stringstream sin_withoutDD(s);
	sin_withoutDD << "kernel_sin_withoutDD";
	sin_withoutDD << vectorSize;
	//dumpPTXCode(context,program,sin_withoutDD.str().c_str());
	// Create the cKermel_kernel_sin_withoutDD
	kernel[1]  = clCreateKernel(program, sin_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_sin_withoutDD)");

	std::stringstream asin_withDD(s);
	asin_withDD << "kernel_asin_withDD";
	asin_withDD << vectorSize;
	//dumpPTXCode(context,program,asin_withDD.str().c_str());
	// Create the cKermel_kernel_asin_withDD
	kernel[2]  = clCreateKernel(program, asin_withDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_asin_withDD)");

	std::stringstream asin_withoutDD(s);
	asin_withoutDD << "kernel_asin_withoutDD";
	asin_withoutDD << vectorSize;
	//dumpPTXCode(context,program,asin_withoutDD.str().c_str());
	// Create the cKermel_kernel_asin_withoutDD
	kernel[3]  = clCreateKernel(program, asin_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_asin_withoutDD)");

	std::stringstream cos_withDD(s);
	cos_withDD << "kernel_cos_withDD";
	cos_withDD << vectorSize;
	//dumpPTXCode(context,program,cos_withDD.str().c_str());
	// Create the cKermel_kernel_cos_withDD
	kernel[4]  = clCreateKernel(program, cos_withDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_cos_withDD)");

	std::stringstream cos_withoutDD(s);
	cos_withoutDD << "kernel_cos_withoutDD";
	cos_withoutDD << vectorSize;
	//dumpPTXCode(context,program,cos_withoutDD.str().c_str());
	// Create the cKermel_kernel_cos_withoutDD
	kernel[5]  = clCreateKernel(program, cos_withoutDD.str().c_str(), &status);
	CHECK_OPENCL_ERROR(status, "clCreateKernel failed.(kernel_cos_withoutDD)");

	return SDK_SUCCESS;
}

int MathBenchmark::setup() {
	//if (setupMemory() != SDK_SUCCESS)
	//	return SDK_FAILURE;

	int timer = sampleCommon->createTimer();
	sampleCommon->resetTimer(timer);
	sampleCommon->startTimer(timer);

	int status = setupCL();
	if (status != SDK_SUCCESS) {
		if (status == SDK_EXPECTED_FAILURE)
			return SDK_EXPECTED_FAILURE;

		return SDK_FAILURE;
	}

	sampleCommon->stopTimer(timer);
	setupTime = (cl_double) sampleCommon->readTimer(timer);
	return SDK_SUCCESS;
}

double MathBenchmark::executionTime(cl_event &event) {
	cl_ulong start, end;
	cl_int status = 0;

	status = clGetEventProfilingInfo(event, CL_PROFILING_COMMAND_END,
			sizeof(cl_ulong), &end, NULL);
	status |= clGetEventProfilingInfo(event, CL_PROFILING_COMMAND_START,
			sizeof(cl_ulong), &start, NULL);

	CHECK_OPENCL_ERROR(status, "clGetEventProfilingInfo failed. (event)");

	return (double) 1.0e-6 * (end - start); // convert nanoseconds to min seconds on return
}

int MathBenchmark::runCLKernels() {
	cl_int status = 0;
	size_t WorkDim = 1;
	size_t szGlobalWorkSize = globalThreads;
	size_t szLocalWorkSize = localThreads;
	size_t szForNum = repeat;

	//create event to record time
	cl_event event_sin_withDD, event_sin_withoutDD;
	cl_event event_asin_withDD, event_asin_withoutDD;
	cl_event event_cos_withDD, event_cos_withoutDD;

	//Create Variable for story result reading from device to host
	void *sin_withDD_num, *sin_withoutDD_num;
	void *asin_withDD_num, *asin_withoutDD_num;
	void *cos_withDD_num, *cos_withoutDD_num;

	sin_withDD_num = (void *) malloc(sizeof(cl_float));
	sin_withoutDD_num = (void *) malloc(sizeof(cl_float));
	asin_withDD_num = (void *) malloc(sizeof(cl_float));
	asin_withoutDD_num = (void *) malloc(sizeof(cl_float));
	cos_withDD_num = (void *) malloc(sizeof(cl_float));
	cos_withoutDD_num = (void *) malloc(sizeof(cl_float));

	float sin_withDD_maxGflops = 0.0;
	float sin_withoutDD_maxGflops = 0.0;
	float asin_withDD_maxGflops = 0.0;
	float asin_withoutDD_maxGflops = 0.0;
	float cos_withDD_maxGflops = 0.0;
	float cos_withoutDD_maxGflops = 0.0;

	//create buffer

	cl_mem result_sin_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_sin_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_asin_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_asin_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_cos_withDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);
	cl_mem result_cos_withoutDD = clCreateBuffer(context, CL_MEM_READ_WRITE,
			sizeof(float), NULL, NULL);

	//set kernel_sin_withDD Argument
	status |= clSetKernelArg(kernel[0] , 0, sizeof(cl_mem),
			(void*) &result_sin_withDD);
	status |= clSetKernelArg(kernel[0] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_sin_withDD)");
	
	//set kernel_sin_withoutDD Argument
	status |= clSetKernelArg(kernel[1] , 0, sizeof(cl_mem),
			(void*) &result_sin_withoutDD);
	status |= clSetKernelArg(kernel[1] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_sin_withoutDD)");

	//set kernel_asin_withDD Argument
	status |= clSetKernelArg(kernel[2] , 0, sizeof(cl_mem),
			(void*) &result_asin_withDD);
	status |= clSetKernelArg(kernel[2] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_asin_withDD)");

	//set kernel_asin_withoutDD Argument
	status |= clSetKernelArg(kernel[3] , 0, sizeof(cl_mem),
			(void*) &result_asin_withoutDD);
	status |= clSetKernelArg(kernel[3] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_asin_withoutDD)");

	//set kernel_cos_withDD Argument
	status |= clSetKernelArg(kernel[4] , 0, sizeof(cl_mem),
			(void*) &result_cos_withDD);
	status |= clSetKernelArg(kernel[4] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_cos_withDD)");

	//set kernel_cos_withoutDD Argument
	status |= clSetKernelArg(kernel[5] , 0, sizeof(cl_mem),
			(void*) &result_cos_withoutDD);
	status |= clSetKernelArg(kernel[5] , 1, sizeof(size_t), (void*) &szForNum);
	CHECK_OPENCL_ERROR(status, "clSetKernelArg failed. (kernel_cos_withoutDD)");
	




	int i = 0;
	int gws, lws;
	for (gws = 1024; gws <= 65536 * 8; gws *= 2)
		for (lws = 64; lws <= 256; lws *= 2) {
			
			float executionTime_sin_withDD_max = 0.0;
			float executionTime_sin_withDD_avg = 0.0;
			float executionTime_sin_withDD_min = 999999999.0;

			float executionTime_sin_withoutDD_max = 0.0;
			float executionTime_sin_withoutDD_avg = 0.0;
			float executionTime_sin_withoutDD_min = 999999999.0;

			float executionTime_asin_withDD_max = 0.0;
			float executionTime_asin_withDD_avg = 0.0;
			float executionTime_asin_withDD_min = 999999999.0;

			float executionTime_asin_withoutDD_max = 0.0;
			float executionTime_asin_withoutDD_avg = 0.0;
			float executionTime_asin_withoutDD_min = 999999999.0;

			float executionTime_cos_withDD_max = 0.0;
			float executionTime_cos_withDD_avg = 0.0;
			float executionTime_cos_withDD_min = 999999999.0;

			float executionTime_cos_withoutDD_max = 0.0;
			float executionTime_cos_withoutDD_avg = 0.0;
			float executionTime_cos_withoutDD_min = 999999999.0;


			szGlobalWorkSize = gws;
			szLocalWorkSize = lws;
			printf("-----------------------------------------------------\n");
			printf("Set gws = %d , lws = %d\n", gws, lws);
			
			//launch kernel_sin_withDD
			if (!strcmp(kernelname.c_str(), "sin_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_sin_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[0] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_sin_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_sin
					status = clWaitForEvents(1, &event_sin_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_sin_withDD, end_sin_withDD;
					status = clGetEventProfilingInfo(event_sin_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_sin_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_sin_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_sin_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_sin_withDD = (end_sin_withDD
							- start_sin_withDD);
					if (executionTime_sin_withDD_max
							< executionTime_sin_withDD) {
						executionTime_sin_withDD_max = executionTime_sin_withDD;
					}
					if (executionTime_sin_withDD_min
							> executionTime_sin_withDD) {
						executionTime_sin_withDD_min = executionTime_sin_withDD;
					}
					executionTime_sin_withDD_avg += executionTime_sin_withDD;

				}
				executionTime_sin_withDD_avg = (executionTime_sin_withDD_avg
						- executionTime_sin_withDD_max
						- executionTime_sin_withDD_min) / (iterations - 2);
				size_t time_sin_withDD=0;
				if(vectorSize==1||vectorSize==2)
					time_sin_withDD= 512 * szForNum * vectorSize;
				if(vectorSize==4||vectorSize==8)
					time_sin_withDD= 128 * szForNum * vectorSize;
				if(vectorSize==16)
					time_sin_withDD= 32 * szForNum * vectorSize;
				float Gflops_sin_withDD =
						(time_sin_withDD * szGlobalWorkSize)
								/ executionTime_sin_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_sin_withDD,
						CL_TRUE, 0, sizeof(cl_mem), sin_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-sin_withDD : %f\n", Gflops_sin_withDD);
				printf("Result-sin_withDD   :   %f\n\n", *((float*)sin_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_sin_withDD > sin_withDD_maxGflops)
						sin_withDD_maxGflops = Gflops_sin_withDD;
			}

			//launch kernel_sin_withoutDD
			if (!strcmp(kernelname.c_str(), "sin_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_sin_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[1] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_sin_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_sin
					status = clWaitForEvents(1, &event_sin_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_sin_withoutDD, end_sin_withoutDD;
					status = clGetEventProfilingInfo(event_sin_withoutDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_sin_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_sin_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_sin_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_sin_withoutDD = (end_sin_withoutDD
							- start_sin_withoutDD);
					if (executionTime_sin_withoutDD_max
							< executionTime_sin_withoutDD) {
						executionTime_sin_withoutDD_max = executionTime_sin_withoutDD;
					}
					if (executionTime_sin_withoutDD_min
							> executionTime_sin_withoutDD) {
						executionTime_sin_withoutDD_min = executionTime_sin_withoutDD;
					}
					executionTime_sin_withoutDD_avg += executionTime_sin_withoutDD;

				}
				executionTime_sin_withoutDD_avg = (executionTime_sin_withoutDD_avg
						- executionTime_sin_withoutDD_max
						- executionTime_sin_withoutDD_min) / (iterations - 2);
				size_t time_sin_withoutDD=0;
				if(vectorSize==1||vectorSize==2||vectorSize==4||vectorSize==8)
					time_sin_withoutDD= 10*7 * szForNum * vectorSize;
				if(vectorSize==16)
					time_sin_withoutDD= 3*7  * szForNum * vectorSize;
				float Gflops_sin_withoutDD =
						(time_sin_withoutDD * szGlobalWorkSize)
								/ executionTime_sin_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_sin_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), sin_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-sin_withoutDD : %f\n", Gflops_sin_withoutDD);
				printf("Result-sin_withoutDD   :   %f\n\n", *((float*)sin_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_sin_withoutDD > sin_withoutDD_maxGflops)
						sin_withoutDD_maxGflops = Gflops_sin_withoutDD;
			}

			//launch kernel_asin_withDD
			if (!strcmp(kernelname.c_str(), "asin_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_asin_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[2] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_asin_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_asin
					status = clWaitForEvents(1, &event_asin_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_asin_withDD, end_asin_withDD;
					status = clGetEventProfilingInfo(event_asin_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_asin_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_asin_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_asin_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_asin_withDD = (end_asin_withDD
							- start_asin_withDD);
					if (executionTime_asin_withDD_max
							< executionTime_asin_withDD) {
						executionTime_asin_withDD_max = executionTime_asin_withDD;
					}
					if (executionTime_asin_withDD_min
							> executionTime_asin_withDD) {
						executionTime_asin_withDD_min = executionTime_asin_withDD;
					}
					executionTime_asin_withDD_avg += executionTime_asin_withDD;

				}
				executionTime_asin_withDD_avg = (executionTime_asin_withDD_avg
						- executionTime_asin_withDD_max
						- executionTime_asin_withDD_min) / (iterations - 2);
				size_t time_asin_withDD=0;
				if(vectorSize==1)
					time_asin_withDD= 512 * szForNum * vectorSize;
				if(vectorSize==2||vectorSize==4||vectorSize==8||vectorSize==16)
					time_asin_withDD= 64 * szForNum * vectorSize;
				float Gflops_asin_withDD =
						(time_asin_withDD * szGlobalWorkSize)
								/ executionTime_asin_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_asin_withDD,
						CL_TRUE, 0, sizeof(cl_mem), asin_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-asin_withDD : %f\n", Gflops_asin_withDD);
				printf("Result-asin_withDD   :   %f\n\n", *((float*)asin_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_asin_withDD > asin_withDD_maxGflops)
						asin_withDD_maxGflops = Gflops_asin_withDD;
			}

			//launch kernel_asin_withoutDD
			if (!strcmp(kernelname.c_str(), "asin_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_asin_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[3] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_asin_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_asin
					status = clWaitForEvents(1, &event_asin_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_asin_withoutDD, end_asin_withoutDD;
					status = clGetEventProfilingInfo(event_asin_withoutDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_asin_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_asin_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_asin_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_asin_withoutDD = (end_asin_withoutDD
							- start_asin_withoutDD);
					if (executionTime_asin_withoutDD_max
							< executionTime_asin_withoutDD) {
						executionTime_asin_withoutDD_max = executionTime_asin_withoutDD;
					}
					if (executionTime_asin_withoutDD_min
							> executionTime_asin_withoutDD) {
						executionTime_asin_withoutDD_min = executionTime_asin_withoutDD;
					}
					executionTime_asin_withoutDD_avg += executionTime_asin_withoutDD;

				}
				executionTime_asin_withoutDD_avg = (executionTime_asin_withoutDD_avg
						- executionTime_asin_withoutDD_max
						- executionTime_asin_withoutDD_min) / (iterations - 2);
				size_t time_asin_withoutDD=0;
				if(vectorSize==1)
					time_asin_withoutDD= 10000 * 8 * vectorSize;
				if(vectorSize==2)
					time_asin_withoutDD= 5000 * 8 * vectorSize;
				if(vectorSize==4)
					time_asin_withoutDD= 2500 * 8 * vectorSize;
				if(vectorSize==8)
					time_asin_withoutDD= 500 * 8 * vectorSize;
				if(vectorSize==16)
					time_asin_withoutDD= 200 * 8 * vectorSize;
				float Gflops_asin_withoutDD =
						(time_asin_withoutDD * szGlobalWorkSize)
								/ executionTime_asin_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_asin_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), asin_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-asin_withoutDD : %f\n", Gflops_asin_withoutDD);
				printf("Result-asin_withoutDD   :   %f\n\n", *((float*)asin_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_asin_withoutDD > asin_withoutDD_maxGflops)
						asin_withoutDD_maxGflops = Gflops_asin_withoutDD;

			}

			//launch kernel_cos_withDD
			if (!strcmp(kernelname.c_str(), "cos_withDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_cos_withDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[4] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_cos_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_cos
					status = clWaitForEvents(1, &event_cos_withDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_cos_withDD, end_cos_withDD;
					status = clGetEventProfilingInfo(event_cos_withDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_cos_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_cos_withDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_cos_withDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_cos_withDD = (end_cos_withDD
							- start_cos_withDD);
					if (executionTime_cos_withDD_max
							< executionTime_cos_withDD) {
						executionTime_cos_withDD_max = executionTime_cos_withDD;
					}
					if (executionTime_cos_withDD_min
							> executionTime_cos_withDD) {
						executionTime_cos_withDD_min = executionTime_cos_withDD;
					}
					executionTime_cos_withDD_avg += executionTime_cos_withDD;

				}
				executionTime_cos_withDD_avg = (executionTime_cos_withDD_avg
						- executionTime_cos_withDD_max
						- executionTime_cos_withDD_min) / (iterations - 2);
				size_t time_cos_withDD=0;
				if(vectorSize==1||vectorSize==2||vectorSize==4||vectorSize==8)
					time_cos_withDD= 64 * szForNum * vectorSize;
				if(vectorSize==16)
					time_cos_withDD= 16 * szForNum * vectorSize;
				float Gflops_cos_withDD =
						(time_cos_withDD * szGlobalWorkSize)
								/ executionTime_cos_withDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_cos_withDD,
						CL_TRUE, 0, sizeof(cl_mem), cos_withDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-cos_withDD : %f\n", Gflops_cos_withDD);
				printf("Result-cos_withDD   :   %f\n\n", *((float*)cos_withDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_cos_withDD > cos_withDD_maxGflops)
						cos_withDD_maxGflops = Gflops_cos_withDD;
			}

			//launch kernel_cos_withoutDD
			if (!strcmp(kernelname.c_str(), "cos_withoutDD")
					|| !strcmp(kernelname.c_str(), "all_kernels")) {
				std::cout << "KERNEL NAME:" <<kernelname.c_str()<< vectorSize <<std::endl;
				printf("Begin to launch kernel_cos_withoutDD\n");
				for (i = 0; i < iterations; i++) {
					status = clEnqueueNDRangeKernel(commandQueue, kernel[5] ,
							WorkDim, NULL, &szGlobalWorkSize, &szLocalWorkSize,
							0, NULL, &event_cos_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					//record time kernel_cos
					status = clWaitForEvents(1, &event_cos_withoutDD);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					cl_ulong start_cos_withoutDD, end_cos_withoutDD;
					status = clGetEventProfilingInfo(event_cos_withoutDD,
							CL_PROFILING_COMMAND_START, sizeof(cl_ulong),
							&start_cos_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");
					status = clGetEventProfilingInfo(event_cos_withoutDD,
							CL_PROFILING_COMMAND_END, sizeof(cl_ulong),
							&end_cos_withoutDD, NULL);
					CHECK_OPENCL_ERROR(status,
							"clEnqueueNDRangeKernel failed. (kernel_none)");

					float executionTime_cos_withoutDD = (end_cos_withoutDD
							- start_cos_withoutDD);
					if (executionTime_cos_withoutDD_max
							< executionTime_cos_withoutDD) {
						executionTime_cos_withoutDD_max = executionTime_cos_withoutDD;
					}
					if (executionTime_cos_withoutDD_min
							> executionTime_cos_withoutDD) {
						executionTime_cos_withoutDD_min = executionTime_cos_withoutDD;
					}
					executionTime_cos_withoutDD_avg += executionTime_cos_withoutDD;

				}
				executionTime_cos_withoutDD_avg = (executionTime_cos_withoutDD_avg
						- executionTime_cos_withoutDD_max
						- executionTime_cos_withoutDD_min) / (iterations - 2);
				size_t time_cos_withoutDD=0;
				if(vectorSize==1||vectorSize==2||vectorSize==4||vectorSize==8)
					time_cos_withoutDD= 10*7 * szForNum * vectorSize;
				if(vectorSize==16)
					time_cos_withoutDD= 3*7 * szForNum * vectorSize;
				float Gflops_cos_withoutDD =
						(time_cos_withoutDD * szGlobalWorkSize)
								/ executionTime_cos_withoutDD_avg;
				status = clEnqueueReadBuffer(commandQueue, result_cos_withoutDD,
						CL_TRUE, 0, sizeof(cl_mem), cos_withoutDD_num, NULL, NULL,
						NULL);
				CHECK_OPENCL_ERROR(status,
						"clEnqueueNDRangeKernel failed. (kernel_none)");
				printf("GFLOPs-cos_withoutDD : %f\n", Gflops_cos_withoutDD);
				printf("Result-cos_withoutDD   :   %f\n\n", *((float*)cos_withoutDD_num));
				printf("-----------------------------------------------------\n");
				if (Gflops_cos_withoutDD > cos_withoutDD_maxGflops)
						cos_withoutDD_maxGflops = Gflops_cos_withoutDD;
			}

	}	
	printf("sin_withDD_maxGflops = %f\n",sin_withDD_maxGflops);
	printf("sin_withoutDD_maxGflops = %f\n",sin_withoutDD_maxGflops);
	printf("asin_withDD_maxGflops = %f\n",asin_withDD_maxGflops);
	printf("asin_withoutDD_maxGflops = %f\n",asin_withoutDD_maxGflops);
	printf("cos_withDD_maxGflops = %f\n",cos_withDD_maxGflops);
	printf("cos_withoutDD_maxGflops = %f\n",cos_withoutDD_maxGflops);

	return SDK_SUCCESS;
}

int MathBenchmark::run() {
/*	
	 // Warm up
	 for(int i = 0; i < 2 && iterations != 1; i++)
	 {
	 // Arguments are set and execution call is enqueued on command buffer
	 if(runCLKernels() != SDK_SUCCESS)
	 return SDK_FAILURE;
	 }
*/	
	int timer = sampleCommon->createTimer();
	sampleCommon->resetTimer(timer);
	sampleCommon->startTimer(timer);
	if (runCLKernels() != SDK_SUCCESS)
		return SDK_FAILURE;
	sampleCommon->stopTimer(timer);
	totalKernelTime = (double) sampleCommon->readTimer(timer);

	//if(!quiet) {
	//	sampleCommon->printArray<cl_float>("Output", eigenIntervals[1], 2*length, 1);
	//}

	return SDK_SUCCESS;
}

int MathBenchmark::cleanup() {
	// Releases OpenCL resources (Context, Memory etc.)
	cl_int status;

	for (int i = 0; i < NUM_KERNELS; i++) {
		status = clReleaseKernel(kernel[i]);
		CHECK_OPENCL_ERROR(status, "clReleaseKernel failed.");
	}

	status = clReleaseProgram(program);
	CHECK_OPENCL_ERROR(status, "clReleaseProgram failed.(program)");

	status = clReleaseCommandQueue(commandQueue);
	CHECK_OPENCL_ERROR(status, "clReleaseCommandQueue failed.(commandQueue)");

	status = clReleaseContext(context);
	CHECK_OPENCL_ERROR(status, "clReleaseContext failed.(context)");

	FREE(devices);
	return SDK_SUCCESS;
}

int MathBenchmark::verifyResults() {
	return SDK_SUCCESS;
}

void MathBenchmark::printStats() {
	std::string strArray[3] = { "DiagonalLength", "Time(sec)",
			"[Transfer+Kernel]Time(sec)" };
	std::string stats[3];

	totalTime = setupTime + totalKernelTime;

	stats[0] = sampleCommon->toString(1410, std::dec);
	stats[1] = sampleCommon->toString(totalTime, std::dec);
	stats[2] = sampleCommon->toString(totalKernelTime, std::dec);

	this->Benchmark::printStats(strArray, stats, 3);
}

int main(int argc, char *argv[]) {
	cl_int status = 0;

	MathBenchmark clMath("Math1 benchmark");

	if (clMath.initialize() != SDK_SUCCESS)
		return SDK_FAILURE;

	if (clMath.parseCommandLine(argc, argv) != SDK_SUCCESS)
		return SDK_FAILURE;

	status = clMath.setup();
	if (status != SDK_SUCCESS) {
		if (status == SDK_EXPECTED_FAILURE)
			return SDK_SUCCESS;

		return SDK_FAILURE;
	}

	if (clMath.run() != SDK_SUCCESS)
		return SDK_FAILURE;

	if (clMath.verifyResults() != SDK_SUCCESS)
		return SDK_FAILURE;

	if (clMath.cleanup() != SDK_SUCCESS)
		return SDK_FAILURE;

	clMath.printStats();

	return SDK_SUCCESS;
}


